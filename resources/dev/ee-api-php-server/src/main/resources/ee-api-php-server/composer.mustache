{
    "name": "{{gitUserId}}/{{gitRepoId}}",
    {{#artifactVersion}}
    "version": "{{.}}",
    {{/artifactVersion}}
    "description": "{{description}}",
    "keywords": [
        "openapi",
        "api",
        "event-engine",
        "api-platform",
        "symfony"
    ],
    {{#homePage}}
    "homepage": "{{.}}",
    {{/homePage}}
    "authors": [
        {
            "name": "openapi-generator-server-stub",
            "homepage": "https://github.com/arnedesmedt/api-platform-event-engine-server-stub"
        }
    ],
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "arnedesmedt/api-platform-event-engine-bundle": "~0.1",
        "symfony/console": "6.1.*",
        "symfony/dotenv": "6.1.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.1.*",
        "symfony/runtime": "6.1.*",
        "symfony/yaml": "6.1.*"{{#workers}},
        "symfony/messenger": "6.1.*"{{/workers}}
    },
    "require-dev": {
        "arnedesmedt/api-platform-event-engine-server-stub": "dev-master",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "~3.0",
        "phpstan/phpstan": "~1.0",
        "phpro/grumphp": "~1.0",
        "rector/rector": "~0.1",
        "doctrine/coding-standard": "~11.0"
    },
    "repositories": {
        "arnedesmedt/api-platform-event-engine-server-stub": {
            "type": "path",
            "url": "../../../php/packages/api-platform-event-engine-server-stub"
        }
    },
    "autoload": {
        "psr-4": {
            "{{escapedInvokerPackage}}\\" : "{{srcBasePath}}/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "{{escapedInvokerPackage}}\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.1.*"
        }
    }
    "minimum-stability": "dev",
    "prefer-stable": true
}