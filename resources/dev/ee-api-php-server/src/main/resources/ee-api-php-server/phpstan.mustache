parameters:
    level: max
    paths:
        - src
    ignoreErrors:
        - '#^Static method (.*)::arrayPropItemTypeMap\(\) is unused.$#'
        - '#^Static method (.*)::__allowNestedSchema\(\) is unused.$#'
services:
	-
		class: ADS\JsonImmutableObjects\PhpStan\PropertiesExtension
		tags:
			- phpstan.properties.readWriteExtension
